% T1 Encoding with Lining Figures
%
% LIGKERN ff i =: ffi ; ff l =: ffl ;
% LIGKERN f b =: f_b ; ff b =: f_f_b ;
% LIGKERN f h =: f_h ; ff h =: f_f_h ;
% LIGKERN f j =: f_j ; ff j =: f_f_j ;
% LIGKERN f t =: f_t ; ff t =: f_f_t ;
% LIGKERN T h =: T_h ;
% LIGKERN space l =: lslash ; space L =: Lslash ;
% LIGKERN question quoteleft =: questiondown ; exclam quoteleft =: exclamdown ;
% LIGKERN hyphen hyphen =: endash ; endash hyphen =: emdash ;
% LIGKERN quoteleft quoteleft =: quotedblleft ;
% LIGKERN quoteright quoteright =: quotedblright ;
% LIGKERN comma comma =: quotedblbase ; less less =: guillemotleft ;
% LIGKERN greater greater =: guillemotright ;
/MinionPro-SwashT1Encoding [          % now 256 chars follow
% 0x00
  /grave /acute /circumflex /tilde /dieresis /hungarumlaut /ring /caron
  /breve /macron /dotaccent /cedilla
  /ogonek /quotesinglbase /guilsinglleft /guilsinglright
% 0x10
  /quotedblleft /quotedblright /quotedblbase /guillemotleft 
  /guillemotright /endash /emdash
  /f_b % /compworkmark
  /f_f_b %/perthousandzero
  /dotlessi
  /f_f_h % /dotlessj
  /ff /fi /fl /ffi /ffl
% 0x20
  /f_h % /visualspace
  /exclam /quotedbl /numbersign
  /dollar /percent /ampersand /quoteright
  /parenleft /parenright /asterisk /plus /comma /hyphen /period /slash
% 0x30
  /uniF639 /uniF6DC /uniF63A /uniF63B /uniF63C /uniF63D /uniF63E /uniF63F
  /uniF640 /uniF641 /colon /semicolon /less /equal /greater /question
% 0x40
  /at /A.swash /B.swash /C.swash /D.swash /E.swash /F.swash /G.swash
  /H.swash /I.swash /J.swash /K.swash /L.swash /M.swash /N.swash /O.swash
% 0x50
  /P.swash /Q.swash /R.swash /S.swash /T.swash /U.swash /V.swash /W.swash
  /X.swash /Y.swash /Z.swash /bracketleft /backslash /bracketright /asciicircum /underscore
% 0x60
  /quoteleft /a /b /c /d /e /f /g /h /i /j /k /l /m /n /o
% 0x70
  /p /q /r /s /t /u /v /w
  /x /y /z /braceleft /bar /braceright /asciitilde /hyphen
% 0x80
  /Abreve.swash /Aogonek.swash /Cacute.swash /Ccaron.swash /Dcaron.swash /Ecaron.swash /Eogonek.swash /Gbreve.swash
  /Lacute.swash /Lcaron.swash /Lslash /Nacute.swash /Ncaron.swash
  /T_h.swash % /Ng
  /Ohungarumlaut.swash /Racute.swash
% 0x90
  /Rcaron.swash /Sacute.swash /Scaron.swash /Scedilla.swash
  /Tcaron.swash
  /f_f_j % /Tcedilla.swash
  /Uhungarumlaut.swash /Uring.swash
  /Ydieresis.swash /Zacute.swash /Zcaron.swash /Zdotaccent.swash /IJ.swash /Idotaccent.swash
  /f_j % /dbar
  /section
% 0xA0
  /abreve /aogonek /cacute /ccaron /dcaron /ecaron /eogonek /gbreve
  /lacute /lcaron /lslash /nacute /ncaron 
  /f_f_k % /ng
  /ohungarumlaut /racute
% 0xB0
  /rcaron /sacute /scaron /scedilla
  /f_k % /tquoteright
  /f_f_t % /tcedilla
  /uhungarumlaut /uring
  /ydieresis /zacute /zcaron /zdotaccent
  /ij /exclamdown /questiondown /sterling
% 0xC0
  /Agrave.swash /Aacute.swash /Acircumflex.swash /Atilde.swash /Adieresis.swash /Aring.swash /AE /Ccedilla.swash
  /Egrave.swash /Eacute.swash /Ecircumflex.swash /Edieresis.swash
  /Igrave.swash /Iacute.swash /Icircumflex.swash /Idieresis.swash
% 0xD0
  /Eth.swash /Ntilde.swash /Ograve.swash /Oacute.swash /Ocircumflex.swash /Otilde.swash /Odieresis.swash /OE
  /Oslash.swash /Ugrave.swash /Uacute.swash /Ucircumflex.swash /Udieresis.swash /Yacute.swash /Thorn
  /f_t % /Germandbls
% 0xE0
  /agrave /aacute /acircumflex /atilde /adieresis /aring /ae /ccedilla
  /egrave /eacute /ecircumflex /edieresis
  /igrave /iacute /icircumflex /idieresis
% 0xF0
  /eth /ntilde /ograve /oacute /ocircumflex /otilde /odieresis /oe
  /oslash /ugrave /uacute /ucircumflex /udieresis /yacute /thorn /germandbls
] def

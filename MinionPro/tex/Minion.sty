\def\varintdots@{\mkern-5mu%
  \mathchoice{\mkern-1mu\@cdots\mkern-1mu}%
  {{\cdotp}{\cdotp}{\cdotp}}%
  {{\cdotp}{\cdotp}{\cdotp}}%
  {{\cdotp}{\cdotp}{\cdotp}\mkern1mu}%

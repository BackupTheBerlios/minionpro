\def\shiftedvarintop{%
  \mathop{%
    \mathchoice
      {\mkern-2mu\vardintop\mkern2mu}
      {\mkern0.75mu\vartintop\mkern-0.75mu}
      {\mkern0.75mu\vartintop\mkern-0.75mu}
      {\mkern0.75mu\vartintop\mkern-0.75mu}}}
\def\varintop{\mathop{\mathchoice{\vardintop}{\vartintop}{\vartintop}{\vartintop}}}
\def\varint{\mkern4mu\DOTSI\overstrike\shiftedvarintop\partialvartint\ilimits@}
\def\variint{\mkern4mu\DOTSI\overstrike{\shiftedvarintop\intkern@\shiftedvarintop}%
                              {\mkern6mu\partialvartint}\ilimits@}
\def\variiint{\mkern4mu\DOTSI\overstrike{\shiftedvarintop\intkern@\shiftedvarintop\intkern@\shiftedvarintop}%
                               {\mkern12mu\partialvartint}\ilimits@}
\def\variiiint{\DOTSI\overstrike
  {\shiftedvarintop\intkern@\shiftedvarintop\intkern@\shiftedvarintop\intkern@\shiftedvarintop}%
  {\mkern18mu\partialvartint}\ilimits@}
\def\varlandupint{\DOTSI\overstrike\shiftedvarintop\partialvartlandupint\ilimits@}
\def\varlanddownint{\DOTSI\overstrike\shiftedvarintop\partialvartlanddownint\ilimits@}
\def\varstrokedint{\DOTSI\overstrike\shiftedvarintop\partialvartstrokedint\ilimits@}
\def\varoint{\DOTSI\overstrike\shiftedvarintop\partialvartoint\ilimits@}
\def\varoiint{\DOTSI\overstrike{\shiftedvarintop\intkern@\shiftedvarintop}\partialvartoiint\ilimits@}
\def\varrcirclerightint{\DOTSI\overstrike\shiftedvarintop\partialvartrcirclerightint\ilimits@}
\def\varlcirclerightint{\DOTSI\overstrike\shiftedvarintop\partialvartlcirclerightint\ilimits@}
